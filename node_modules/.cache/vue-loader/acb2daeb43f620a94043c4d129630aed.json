      getPlanMessage() {\n                this.$http({\n                    url: 'PlanMessage',\n                }).then(res => {\n                    this.Owner = res.Owner;\n                    this.WorkPlanName = res.WorkPlanName;\n                    this.ReleaseTime = res.ReleaseTime;\n                });\n            },\n            //获取的百分比\n            getPercentage() {\n                this.$http({\n                    url: 'GetPercentage',\n                }).then(res => {\n                    c\n                    this.Percentage = res;\n                });\n            },\n            getDataCenterData() {\n                this.$http({\n                    url: 'DataCenter',\n                }).then(res => {\n                    this.tableData = res.data;\n\n                    console.log(res)\n                });\n            },\n            openExportEvent () {\n                this.$refs.xTable1.openExport({ isPrint: false })\n            }\n        }\n    };\n</script>\n<style lang=\"scss\" scoped>\n    [v-cloak] {\n        display: none;\n    }\n    html, body {\n        width: 100%;\n        height: 100%;\n    }\n    .wrap {\n        .title, .titleL {\n            height: 30px;\n            line-height: 30px;\n            text-align: right;\n            background-color: #E2E3E5;\n            padding: 6px 10px;\n            -webkit-border-radius: 5px;\n            -moz-border-radius: 5px;\n            border-radius: 5px;\n            font-size: 14px;\n            overflow: hidden;\n        }\n\n        .titleL {\n            text-align: left;\n        }\n\n        .flexBox {\n            display: flex;\n            margin: 20px 0;\n\n            div {\n                flex-grow: 1;\n                margin: 0 12px;\n                -webkit-border-radius: 8px;\n                -moz-border-radius: 8px;\n                border-radius: 8px;\n                padding: 8px;\n                background-color: red;\n                border-bottom: 1px solid